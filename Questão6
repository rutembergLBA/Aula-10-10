import java.util.Scanner; 
public class Questão6{
    public static void main() {
        Scanner scanner = new Scanner (System.in);
        int n1;
        int soma=0;
        
        for (int i = 1; i <= 10; i++){
            System.out.println("Informe seu número:");
            n1 = scanner.nextInt();
            
            soma += n1;
            
        }
        System.out.println("A soma dos números é:" + soma);
    }
}
